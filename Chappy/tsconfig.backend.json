{
	"compilerOptions": {
	  "target": "ES2022",
	  "lib": ["ES2023"],
	  "module": "ESNext",
	  "skipLibCheck": true,
	  "moduleResolution": "node",  
	  "isolatedModules": true,
	  "noEmit": false,
	  "outDir": "./backendDist",
	  "strict": false,
	  "noUnusedLocals": true,
	  "noUnusedParameters": true,
	  "noFallthroughCasesInSwitch": true,
	  "allowSyntheticDefaultImports": true 
	},
	"include": ["backendSrc/**/*"]
  }
  